{
  "name": "yosethegame",
  "version": "1.0.0",
  "description": "",
  "main": "app/start.mjs",
  "type": "module",
  "scripts": {
    "pretest": "npm run prettier:write && npm run ghpages",
    "test": "node --experimental-test-coverage --test-coverage-include=app/**/*.js --test-coverage-exclude=app/**/__tests__/**/*.js --test $(find about app -name '*.test.js')",
    "test:external": "node --experimental-test-coverage --test-coverage-include=app/**/*.js --test-coverage-exclude=app/**/__tests__/**/*.js --test $(find about -name '*.test.js')",
    "test:internal": "node --experimental-test-coverage --test-coverage-exclude=**/__tests__/**/*.js --test $(find app yop -name '*.test.js')",
    "ghpages": "node -e 'import fs from \"node:fs\"; import { scripts } from \"./app/web/assets/scripts.js\"; fs.writeFileSync(\"./app/web/assets/app.js\", scripts);'",
    "start": "node app/server.js",
    "prettier:check": "npx prettier --check .",
    "prettier:write": "npx prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericminio/yosethegame.git"
  },
  "keywords": [
    "tdd",
    "game"
  ],
  "author": "ericminio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ericminio/yosethegame/issues"
  },
  "homepage": "https://github.com/ericminio/yosethegame#readme",
  "dependencies": {
    "pg": "^8.11.0",
    "prettier": "^3.2.5"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^8.2.6",
    "jsdom": "^22.0.0",
    "playwright": "^1.40.0",
    "selenium-webdriver": "^4.14.0"
  }
}
